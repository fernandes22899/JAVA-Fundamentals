public class TicTacToe
{
    public TTT{
        
    }
    
    public static void createBoard(String[][] board){
        for(int r = 0; r < board.length; r++)
        {
            for(int c = 0; c < board[0].length; c++)
            {
                board[r][c] = "~";
            }
        }
    }
    
    public static void main( string [] args){
        String[][] board = new String[3][3];
        TTT t = new TTT();
    }
}